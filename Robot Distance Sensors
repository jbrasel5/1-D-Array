#include <iostream>
using namespace std;

int main()
{
    const int size = 5;

    int distance[size]; //Array Declaration

    // 1) Take 5 readings using a loop

    for (int index = 0; index < size; index++)
    {
        cin >> distance[index];
    }

    // 2) Store first and last into variables

    int first_reading = distance[0];
    int last_reading = distance[size - 1];

    return 0;
}

